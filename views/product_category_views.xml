<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="product_category_form_view_inherit_override" model="ir.ui.view">
        <field name="name">product.category.form.inherit.override</field>
        <field name="model">product.category</field>
        <field name="inherit_id" ref="product.product_category_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='business_unit']" position="after">
                <field name="override_credit_days"/>
            </xpath>
        </field>
    </record>
</odoo>